[*.vb]

# CA1031: Do not catch general exception types
dotnet_diagnostic.CA1031.severity = silent

# IDE0044: Adicionar modificador somente leitura
dotnet_style_readonly_field = false:suggestion

# IDE0051: Remover membros privados não utilizados
dotnet_diagnostic.IDE0051.severity = none

# IDE0002: Simplificar o Acesso de Membro
dotnet_diagnostic.IDE0002.severity = none

# IDE0059: Atribuição desnecessária de um valor
visual_basic_style_unused_value_assignment_preference = unused_local_variable:none

# IDE0060: Remover o parâmetro não utilizado
dotnet_code_quality_unused_parameters = all:none

# IDE0017: Simplificar a inicialização de objeto
dotnet_style_object_initializer = true:none

# IDE0001: Simplificar Nomes
dotnet_diagnostic.IDE0001.severity = silent

# Default severity for analyzer diagnostics with category 'Style'
dotnet_analyzer_diagnostic.category-Style.severity = none
